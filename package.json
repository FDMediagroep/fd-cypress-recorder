    "cross-env": "^5.2.0",
    "tslint": "^5.16.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "@fdmg/fd-buttons": "^1.0.27",
    "@fdmg/fd-inputs": "^1.0.24",
